{
  "name": "ts-boilerplate",
  "version": "1.0.0",
  "description": "Basic Typescript + Node Boilerplate",
  "author": "AvyKween",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev:setup": "wait-on dist/index.js && nodemon dist/index.js",
    "dev": "concurrently \"tsc --watch\" \"yarn dev:setup\""
  },
  "keywords": [
    "typescript"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.37.0",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.3",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "dotenv": "^16.0.3"
  }
}
